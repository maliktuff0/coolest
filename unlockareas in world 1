best going to all areas script 
local TARGET_POSITIONS = {
    Vector3.new(15.9, 423.1, 143.4),
    Vector3.new(41.5, 2663.2, 6.4),
    Vector3.new(77.9, 6862.5, 88.3),
    Vector3.new(16.0, 10146.0, 151.7),
    Vector3.new(37.8, 15972.7, 46.8)
}

local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")

local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local rootPart = character:WaitForChild("HumanoidRootPart")

local TWEEN_SPEED = 1000 -- Blazing fast 1000 studs/sec

local function warpTo(target)
    local tweenInfo = TweenInfo.new(
        (target - rootPart.Position).Magnitude / TWEEN_SPEED,
        Enum.EasingStyle.Linear
    )
    TweenService:Create(rootPart, tweenInfo, {CFrame = CFrame.new(target)}):Play()
end

for i, pos in ipairs(TARGET_POSITIONS) do
    print("Warping to position "..i)
    warpTo(pos)
    wait(0.1) -- Tiny delay between positions
end
print("All positions reached at light speed!")
