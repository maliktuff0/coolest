local worlds = {"Seven Seas", "Blizzard Hills", "Poison Jungle", "Infernite Volcano"}

local function MovementSequence(world)
    local args = {"UnlockWorld", world}
    game:GetService("ReplicatedStorage"):WaitForChild("Shared"):WaitForChild("Framework"):WaitForChild("Network"):WaitForChild("Remote"):WaitForChild("RemoteEvent"):FireServer(unpack(args))
    
    args = {"WorldTeleport", world}
    game:GetService("ReplicatedStorage"):WaitForChild("Shared"):WaitForChild("Framework"):WaitForChild("Network"):WaitForChild("Remote"):WaitForChild("RemoteEvent"):FireServer(unpack(args))

    local player = game:GetService("Players").LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
    local tweenService = game:GetService("TweenService")

    local function TweenMove(cframe, duration)
        tweenService:Create(humanoidRootPart, TweenInfo.new(duration, Enum.EasingStyle.Linear), {CFrame = cframe}):Play()
        task.wait(duration)
    end

    task.wait(0.5)
    TweenMove(humanoidRootPart.CFrame * CFrame.new(0, 0, -100), 2.5)
    TweenMove(humanoidRootPart.CFrame * CFrame.new(15, 0, 0), 0.5)
    TweenMove(humanoidRootPart.CFrame * CFrame.new(0, 0, -45), 1.25)
    
    task.wait(10)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/IdiotHub/Scripts/refs/heads/main/BGSI/main.lua"))()
end

while true do
    for _, world in ipairs(worlds) do
        MovementSequence(world)
        task.wait(3600)
    end
end
