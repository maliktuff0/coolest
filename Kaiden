repeat wait() until game:IsLoaded() and game.Players.LocalPlayer

local chat = game:GetService("TextChatService")
local ads = {
    "JOIN SUPER GARDEN RRDsdT5w (put gg/ before it)",
    "BEST TRADERS @ SUPER GARDEN RRDsdT5w (add gg/)", 
    "PRO TRADING IN SUPER GARDEN RRDsdT5w (gg/ + this)"
}

for i = 1, #ads do
    chat.TextChannels.RBXGeneral:SendAsync(ads[i])
    task.wait(1.2)
end
task.wait(3)
game:GetService("TeleportService"):Teleport(126884695634066)
